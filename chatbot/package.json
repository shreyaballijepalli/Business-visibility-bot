{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "Simple chatbot",
  "main": "app.js",
  "scripts": {
    "test": "npm run lint && npm run test-unit && npm run test-integration-runner",
    "start": "node server.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test-unit": "istanbul cover ./node_modules/mocha/bin/_mocha test/unit",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "author": "Shreya",
  "license": "Apache-2.0",
  "dependencies": {
    "bcryptjs": "*",
    "body-parser": "^1.15.2",
    "connect-flash": "*",
    "cookie-parser": "^1.4.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-handlebars": "*",
    "express-messages": "*",
    "express-session": "*",
    "express-validator": "*",
    "mongodb": "*",
    "mongoose": "*",
    "npm": "^4.1.1",
    "passport": "*",
    "passport-http": "*",
    "passport-local": "*",
    "watson-developer-cloud": "^2.31.1",
    "xlsx-to-json-lc": "^0.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "casperjs": "^1.1.3",
    "codecov": "^1.0.1",
    "eslint": "^2.8.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.13",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": ">= 6.9.x",
    "npm": "> 3.10.x"
  }
}
