    "ejs": "^2.5.6",
    "exec-php": "0.0.3",
    "express": "^4.14.0",
    "express-handlebars": "*",
    "express-messages": "*",
    "express-session": "*",
    "express-validator": "*",
    "fs-extra": "^3.0.1",
    "ibm_bluepages": "^1.1.5",
    "jade": "^1.11.0",
    "latest-version": "^3.1.0",
    "ldapjs": "^1.0.1",
